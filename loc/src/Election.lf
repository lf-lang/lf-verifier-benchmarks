target C
reactor Node0 {
    input in:int
    output out:int
    logical action a(10 msec):int
    state id:int
    state elected:int
    reaction(startup) -> out {=
        self->id = 0;
        lf_set(out, self->id);
    =}
    reaction(a) -> out {=
        lf_set(out, a->value);
    =}
    reaction(in) -> a {=
        if (in->value > self->id) {
            lf_schedule_int(a, 0, in->value);
        } else if (in->value == self->id) {
            self->elected = 1;
        }
    =}
}
