@property(name="exactly_one_elected", tactic="bmc", spec="F[0, 20 msec]((Election_i0_elected + Election_i1_elected + Election_i2_elected) == 1)", expect=true)
main reactor {
    i0 = new Node0()
    i1 = new Node1()
    i2 = new Node2()
    i0.out -> i1.in
    i1.out -> i2.in
    i2.out -> i0.in
}